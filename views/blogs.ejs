<%- layout("layouts/boilerplate") %>

    <body>
        <div class="cont">
            <h3>all blogs</h3>

            <div class="row row-cols-lg-3 row-cols-md-2 row-cols-sm-1">


                <% for(let content of allcontents){ %>
                    <a href="/blogs/showblogs/<%=content._id%>" class="listing-link">




                        <div class="card col">
                            <img src=<%=content.img%> class="card-img-top" alt="..."
                            style="height: 20rem;">
                            <div class="card-img-overlay"></div>
                            <div class="card-body">
                                <p class="card-text">
                                    <b>
                                        <%=content.titel%>
                                    </b><br>

                                </p>

                            </div>
                        </div>

                    </a>

                    <% } %>



            </div>

        </div>

    </body>